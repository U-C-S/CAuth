generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model user_info {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  password  String
  createdAt DateTime  @default(now())
  dob       DateTime?
  about     String?
}

model weather_user_data {
  email           String            @unique
  name            String
  favorite_cities favorite_cities[]
}

model favorite_cities {
  id                     Int                @id @default(autoincrement())
  city                   String
  weather_user_data      weather_user_data? @relation(fields: [weather_user_dataEmail], references: [email])
  weather_user_dataEmail String?
}
